<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MomToBe" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/mom_colorPrimary</item>
        <item name="colorPrimaryDark">@color/mom_colorPrimaryDark</item>
        <item name="colorAccent">@color/mom_colorPrimary</item>
        <item name="android:statusBarColor">@color/mom_colorWhitePure</item>

    </style>

    <style name="Theme.MomToBe.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MomToBe.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MomToBe.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>